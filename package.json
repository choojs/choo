{
  "name": "choo",
  "version": "2.3.1",
  "description": "A 7kb framework for creating sturdy frontend applications",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev -i xhr",
    "test": "standard && npm run deps && NODE_ENV=test tape tests/index.js && ./scripts/test",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover tests/index.js"
  },
  "repository": "yoshuawuyts/choo",
  "keywords": [
    "client",
    "frontend",
    "framework",
    "minimal",
    "composable",
    "tiny"
  ],
  "license": "MIT",
  "dependencies": {
    "global": "^4.3.0",
    "hash-match": "^1.0.2",
    "send-action": "^2.0.2",
    "sheet-router": "^3.1.0",
    "xhr": "^2.2.0",
    "xtend": "^4.0.1",
    "yo-yo": "^1.2.0"
  },
  "devDependencies": {
    "bankai": "^2.0.2",
    "browserify": "^13.0.1",
    "bundle-collapser": "^1.2.1",
    "dependency-check": "^2.5.1",
    "es2020": "^1.0.1",
    "insert-css": "^0.2.0",
    "istanbul": "^0.4.3",
    "server-router": "^2.1.0",
    "sheetify": "^5.0.0",
    "smokestack": "^3.4.1",
    "standard": "^7.1.0",
    "tachyons": "^4.0.0-beta.19",
    "tap-closer": "^1.0.0",
    "tape": "^4.5.1"
  }
}
